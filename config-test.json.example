{
  "agents": {
    "defaults": {
      "workspace": "/root/.nanobot/workspace",
      "model": "anthropic/claude-sonnet-4-5",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 20
    }
  },
  "channels": {
    "slack": {
      "enabled": true,
      "botToken": "xoxb-your-test-bot-token",
      "appLevelToken": "xapp-your-test-app-token",
      "allowFrom": ["U12345678"]
    },
    "telegram": {
      "enabled": false
    },
    "whatsapp": {
      "enabled": false
    },
    "feishu": {
      "enabled": false
    }
  },
  "providers": {
    "anthropic": {
      "apiKey": "sk-ant-your-key"
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "",
        "maxResults": 5
      }
    },
    "exec": {
      "timeout": 60,
      "restrictToWorkspace": false
    }
  }
}
